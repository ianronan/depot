<h1>Administrators</h1>
<% if notice %>
<p id="notice"><%= notice %></p>
<% end %>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
      <tr>
        <td><%= user.name %></td>
        <td><%= link_to image_tag("view.jpg"), user %></td>
        <td><%= link_to image_tag("edit.jpg"), edit_user_path(user) %></td>
        <td><%= link_to image_tag("delete.jpg"), user, method: :delete,
        data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to image_tag("admin.jpg"), new_user_path %>